<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.5.2/css/bootstrap.min.css">
  <link rel="stylesheet" href="/css/template.css">
  <title>Majster</title>
</head>
  <body>


    <ul>
      <% if(typeof errors != 'undefined') { %> 
        <% errors.forEach(error=>{%>
          <li><%= error.message %></li>
        
          
     <% })  %> <% } %>
    </ul>

    <content>
      <form action="/users/AddUser" method="POST">
        <a1>Podaj swoje imię</a1>
        <div>
          <input type="name" id="name" name="name" placeholder="Imię użytkownika" required><br>
        </div>
      
        <a2>Podaj swoje nazwisko </a2>
        <div>
          <input type="surname" id="surname" name="surname" placeholder="Nazwisko użytkownika" required><br>
        </div>
      
        <a2>Podaj swój email </a2>
        <div>
          <input type="email" id="email" name="email" placeholder="Email użytkownika" required>
        </div>

        <a2>Podaj login</a2>
        <div>
          <input type="login" id="login" name="login" placeholder="Login" required>
        </div>
      
        <a2>Podaj hasło</a2>
        <div>
          <input type="password" id="password" name="password" placeholder="Hasło" required>
        </div>

        <a2>Podaj rolę</a2>
        <div>
          <input
            type="text"
            id="role"
            name="role"
            placeholder="Role"
            required
          />
        </div>
      
        <input type="submit" value="Dodaj użytkownika" />
      </form> 
      <button><a href="/users/Dashboard">Powrót do strony głównej</a></button>
      
    </content>


  </body>
</html>
