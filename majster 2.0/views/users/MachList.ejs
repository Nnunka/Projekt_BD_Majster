<%- include('../components/Head')%>

<body>

  <header>
    <%- include('../components/Navbar')%>
  </header>
  
  <table class="table">
    <thead>
      <tr>
        <th scope="col">Nazwa</th>
        <th scope="col">Rodzaj</th>
        <th scope="col">Producent</th>
        <th scope="col">Status</th>
      </tr>
    </thead>
    <tbody>
      <% users.forEach(function(user) { %>
        <tr>
          <td><%= user.name %></td>
          <td><%= user.surname %></td>
          <td><%= user.email %></td>
          <td><%= user.role %></td>
        </tr>
      <% }); %>
    </tbody>
  </table>

  <div class="col-md-12 text-center">
    <button class="btn btn-warning "><a href="/users/AddUser">Dodaj nowego u≈ºytkownika</a></button>
  </div>
</body>